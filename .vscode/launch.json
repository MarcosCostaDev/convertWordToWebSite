    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "debug gulp default",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": true,
            "args": [
                "default"
            ],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "skipFiles": [
                "${workspaceRoot}/node_modules/**/*.js",
            ],
            "env": {}
        },
        {
            "type": "node",
            "request": "launch",
            "name": "debug gulp createMenu",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": true,
            "args": [
                "createMenu"
            ],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "skipFiles": [
                "${workspaceRoot}/node_modules/**/*.js",
            ],
            "env": {}
        },
        {
            "type": "node",
            "request": "launch",
            "name": "debug gulp server",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": true,
            "args": [
                "only-server"
            ],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "skipFiles": [
                "${workspaceRoot}/node_modules/**/*.js",
            ],
            "env": {}
        },
        {
            "type": "node",
            "request": "launch",
            "name": "debug gulp check-access",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": true,
            "args": [
                "check-access"
            ],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "skipFiles": [
                "${workspaceRoot}/node_modules/**/*.js",
            ],
            "env": {}
        }
    ]
}